{
  "name": "LeafletPlayback",
  "version": "0.2.3",
  "author": "Nicholas Hallahan <nick@theoutpost.io>",
  "contributors": [
    "Marius Bieliauskas <@recallfx>"
  ],
  "description": "This is a Leaflet plug-in that plays back points that have a time stamp synchronized to a clock.",
  "homepage": "http://leafletplayback.theoutpost.io/",
  "repository": {
    "type": "git",
    "url": "git@github.com:hallahan/LeafletPlayback.git"
  },
  "main": "dist/LeafletPlayback.js",
  "dependencies": {
    "leaflet": "^1.0.3"
  },
  "devDependencies": {
    "grunt": "~0.4.5",
    "grunt-contrib-concat": "~0.3.0",
    "grunt-contrib-uglify": "~0.9.1",
    "grunt-write-bower-json": "0.0.1"
  },
  "directories": {
    "data": "data/",
    "examples": "examples/",
    "lib": "src/"
  },
  "files": [
    "dist/*",
    "data/*",
    "examples/*"
  ],
  "eslintConfig": {
    "root": true,
    "globals": {
      "L": true
    },
    "env": {
      "commonjs": true,
      "amd": true,
      "node": false
    },
    "extends": "mourner",
    "rules": {
      "no-mixed-spaces-and-tabs": [
        2,
        "smart-tabs"
      ],
      "indent": [
        2,
        "tab",
        {
          "VariableDeclarator": 0
        }
      ],
      "curly": 2,
      "spaced-comment": 2,
      "strict": 0,
      "wrap-iife": 0,
      "key-spacing": 0,
      "consistent-return": 0
    }
  },
  "readmeFilename": "README.md"
}
